            <xml-property name="queryText"><![CDATA[select
	concat_ws(' ', patient.last_name, patient.first_name) as patient_name,
	date_format(patient.birthdate, '%Y-%m-%d') as patient_birthday,
	patient.identifier as patient_no,
	case WHEN patient.is_male=1 then 'M' else 'F' END as patient_sex
