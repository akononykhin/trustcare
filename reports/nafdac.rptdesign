            </list-property>
            <xml-property name="queryText"><![CDATA[select 
	adr_description,
	case when outcome_of_reaction_type=1 then 'X' else '' end as recovered_fully,
	case when outcome_of_reaction_type=2 then 'X' else '' end as congenital_abnormality,
	case when outcome_of_reaction_type=2 then outcome_of_reaction_desc else '' end as congenital_abnormality_desc,
	case when outcome_of_reaction_type=3 then 'X' else '' end as death,
	case when outcome_of_reaction_type=4 then 'X' else '' end as recovered_with_disability,
	case when outcome_of_reaction_type=4 then outcome_of_reaction_desc else '' end as recovered_with_disability_desc,
	case when outcome_of_reaction_type=5 then 'X' else '' end as life_threatening,
	case when outcome_of_reaction_type=5 then outcome_of_reaction_desc else '' end as life_threatening_desc,
	case when outcome_of_reaction_type=6 then 'X' else '' end as other,
	case when outcome_of_reaction_type=6 then outcome_of_reaction_desc else '' end as other_desc,
	case when was_admitted=1 then 'YES' else 'NO' end as was_admitted,
	case when was_hospitalization_prolonged=1 then 'YES' else 'NO' end as was_hospitalization_prolonged,	
	duration_of_admission,
	treatment_of_reaction,
	drug_brand_name,
	drug_generic_name,
	drug_batch_number,
	drug_nafdac_number,
	drug_expiry_name,
	drug_manufactor,
	drug_indication_for_use,
	drug_dosage,
	drug_route_of_administration,
	drug_date_started,
	drug_date_stopped,
	reporter_name,
	reporter_address,
	reporter_profession,
	reporter_contact
	
	
from nafdac
where id_frm_care=?]]></xml-property>
